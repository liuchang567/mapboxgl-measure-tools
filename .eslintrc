{
  "extends": [
    "plugin:vue/essential",
    "@vue/standard"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "plugins": [
    "import"
  ],
  "globals": {
    "document": true
  },
  "rules": {
    "no-duplicate-imports": "off",
    "import/no-duplicates": "error",
    "import/no-commonjs": "error"
  },
  "env": {
    "es6": true,
    "browser": true
  }
}



